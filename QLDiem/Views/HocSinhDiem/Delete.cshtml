@model QLDiem.Models.HocSinhDiem

@{
    ViewData["Title"] = "Quản lý điểm";
    Layout = "~/Views/Shared/MyLayout.cshtml";
}
<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>HocSinhDiem</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Điểm 15 phút
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Diem15p)
        </dd>
        <dt class = "col-sm-2">
            Điểm miệng
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DiemMieng)
        </dd>
        <dt class = "col-sm-2">
            Điểm 1 tiết
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Diem1Tiet)
        </dd>
        <dt class = "col-sm-2">
            Điểm giữa kì
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DiemGk)
        </dd>
        <dt class = "col-sm-2">
            Điểm thi học kỳ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DiemthiHk)
        </dd>
        <dt class = "col-sm-2">
            Điểm trung bình môn
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DiemTbm)
        </dd>
        <dt class = "col-sm-2">
            Mã học sinh
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MahocsinhNavigation.MaHs)
        </dd>
        <dt class = "col-sm-2">
            Mã môn
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MamonNavigation.Mamon)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Mahocsinh" />
        <input type="hidden" asp-for="Kyhoc" />
        <input type="hidden" asp-for="Mamon" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
